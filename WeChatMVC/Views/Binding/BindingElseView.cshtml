
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>BindingElseView</title>
    <script src="~/Scripts/jquery-2.1.4.min.js"></script>
    <link href="~/Content/webuploader.css" rel="stylesheet" />
    <script src="~/Scripts/webuploader.nolog.min.js"></script>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrap.min.js"></script>
</head>
<body>
    <div>
        <div class="col-md-3">
        </div>
        <div class="col-md-6">
            <form method="post" action="binding/else">
                <input type="text" id="openid" name="openid" style="display:none" value="@ViewData["openid"]" />
                <input type="text" id="secret" name="secret" style="display:none" value="@ViewData["secret"]" />
                <div class="panel-group" id="panel-609591">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <a class="panel-title" data-toggle="collapse" data-parent="#panel-609591" href="#panel-element-953429">绑定或修改体育查询密码</a>
                        </div>
                        <div id="panel-element-953429" class="panel-collapse in">
                            <div class="panel-body">
                                <div class="form-group">
                                    <input type="text" name="typwd" class="form-control" id="typwd" placeholder="请输入体育密码">
                                    <p class="help-block">体育密码用来查询晨跑次数</p>
                                </div>
                            </div>
                        </div>
                    </div>



                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <a class="panel-title collapsed" data-toggle="collapse" data-parent="#panel-609591" href="#panel-element-430723">绑定或修改教务查询密码</a>
                        </div>
                        <div id="panel-element-430723" class="panel-collapse collapse">
                            <div class="panel-body">
                                暂时没什么好查的
                            </div>
                        </div>
                    </div>

                </div>
                <div class="col-md-6 pull-left">
                    <div class="modal-footer" align="left">
                        <button type="submit" class="btn btn-info pull-left">提交</button>
                    </div>
                </div>
            </form>
        </div>
        <div class="col-md-3">
        </div>
    </div>
</body>
</html>
